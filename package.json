{
  "description": "",
  "scripts": {
    "test": "ts-node ./scripts/runner.ts",
    "_test": "pnpm jest --runInBand $FILE",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "pnpm compile",
    "pretest": "pnpm compile",
    "posttest": "pnpm lint",
    "publish-cli": "pnpm compile && pnpm publish build/packages/solana-cli-utils",
    "publish-test": "pnpm compile && pnpm publish build/packages/solana-test-utils",
    "publish-all": "pnpm compile && pnpm publish build/packages/solana-cli-utils && pnpm publish build/packages/solana-test-utils"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "gts": "^3.1.1",
    "@solana/web3.js": "^1.54.0",
    "@types/bn.js": "^5.1.1",
    "@types/jest": "^29.2.6",
    "@types/node": "^18.11.18",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">10"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react-native"
      ],
      "allowedVersions": {
        "jest": "28"
      }
    },
    "packageExtensions": {
      "@gokiprotocol/client": {
        "dependencies": {
          "@solana/buffer-layout": "^3.0.0"
        }
      },
      "@saberhq/token-utils": {
