          "@solana/spl-token@0.1.8": "patches/@solana__spl-token@0.1.8.patch"
        }
      },
      "@solana/spl-token@0.1.8": {
        "dependencies": {
          "@solana/buffer-layout": "^3.0.0"
        }
      }
    },
    "patchedDependencies": {
      "@solana/spl-token@0.1.8": "patches/@solana__spl-token@0.1.8.patch"
    }
  }
